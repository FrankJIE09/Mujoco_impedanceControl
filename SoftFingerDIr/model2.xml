<!--    </body>-->
    <body name="softfinger2" pos=".25 -.45 .05">
      <freejoint name="finger2"/>
    <geom  name="softfinger2" type="box" mesh="softfinger" pos="0 0 0.1" rgba="1 1 1 1" size="1" euler="-90 0 0" material="soft_material" solref="0.01 1" solimp=".95 .99 .0001"/>
    </body>
    <body name="box_and_sphere" euler="0 0 -30" pos=".25 -.45 .15">
            <freejoint name="box"/>

            <geom name="red_box" type="box" size=".2 .2 .2" rgba="1 0 0 1" solref="0.01 1" solimp=".95 .99 .0001"/>
